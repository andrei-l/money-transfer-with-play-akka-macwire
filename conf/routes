
POST        /account                           controllers.AccountsController.openAccount()
POST        /account/:id/deposit               controllers.AccountsController.depositMoney(id: Long)
POST        /account/:id/withdraw              controllers.AccountsController.withdrawMoney(id: Long)
POST        /account/:id/transfer-money        controllers.AccountsController.transferMoney(id: Long)

GET         /account/:id                       controllers.AccountsController.loadAccount(id: Long)
